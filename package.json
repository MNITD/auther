  "bugs": {
    "url": "https://github.com/MNITD/auther/issues"
  },
  "homepage": "https://github.com/MNITD/auther#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2"
  }
}
