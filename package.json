{
  "name": "auther",
  "version": "1.0.0",
  "description": "Simple auth server on node for demo purposes",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node auth_server/src/server",
    "prebuild": "rimraf dist",
    "build": "babel auth_server/src --out-dir dist",
    "preserve": "npm run build",
    "serve": "node dist"
  },
